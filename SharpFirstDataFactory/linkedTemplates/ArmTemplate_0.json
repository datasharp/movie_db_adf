										},
										"sink": {
											"name": "Description",
											"type": "String",
											"physicalType": "varchar"
										}
									},
									{
										"source": {
											"name": "Director",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "Director",
											"type": "String",
											"physicalType": "nvarchar"
										}
									},
									{
